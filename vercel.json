{
  "version": 2,
  "builds": [
    {
      "src": "server/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server/index.ts"
    },
    {
      "src": "/static/(.*)",
      "dest": "client/dist/static/"
    },
    {
      "src": "/assets/(.*)",
      "dest": "client/dist/assets/"
    },
    {
      "src": "/@vite/(.*)",
      "dest": "client/dist/@vite/"
    },
    {
      "src": "/",
      "dest": "client/dist/index.html"
    },
    {
      "src": "/(.*)\.(js|css|png|jpg|svg|ico)",
      "dest": "client/dist/."
    },
    {
      "src": "/(.*)",
      "dest": "client/dist/index.html"
    }
  ]
}
