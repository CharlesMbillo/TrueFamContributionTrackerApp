{
  "version": 2,
  "builds": [
    {
      "src": "server/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server/index.ts"
    },
    {
      "src": "/static/(.*)",
      "dest": "client/dist/static/"
    },
    {
      "src": "/assets/(.*)",
      "dest": "client/dist/assets/"
    },
    {
      "src": "/(.*)",
      "dest": "client/dist/"
    }
  ]
}
